[{"update_date": "2014/12/29", "name": "\u56b4\u91cd\u9055\u53cdGMP\u85e5\u5ee0", "the_url": "http://data.fda.gov.tw/frontsite/data/DataAction.do?method=doDetail&infoId=45", "the_type": "\u98a8\u96aa\u7ba1\u7406", "provider": "\u98a8\u96aa\u7ba1\u7406\u7d44", "the_id": "45", "columns": ["\u5ee0\u540d", "\u5730\u5740", "\u67e5\u5ee0\u65e5\u671f", "\u767c\u4f48\u65e5\u671f", "\u985e\u5225"], "desc": "\r\n            \u672c\u8cc7\u6599\u96c6\u70ba\u56b4\u91cd\u9055\u53cdGMP \u85e5\u5ee0\u540d\u55ae\uff0c\u56b4\u91cd\u9055\u53cdGMP\u4e4b\u7f3a\u5931\uff0c\u82e5\u6709\u76f4\u63a5\u6d89\u53ca\u7522\u54c1\u54c1\u8cea\u4e4b\u865e\u4e14\u6d89\u53ca\u6c11\u773e\u7528\u85e5\u5b89\u5168\u8005\uff0c\u9664\u52d2\u4ee4\u5176\u90e8\u5206\u7522\u54c1\u3001\u5291\u578b\u6216\u6574\u5ee0\u505c\u6b62\u751f\u7522\u6216\u4e0d\u5f97\u8ca9\u8ce3\u5916\uff0c\u5c07\u4f9d\u56de\u6536\u4f5c\u696d\u5be6\u65bd\u8981\u9ede\u898f\u5b9a\uff0c\u8981\u6c42\u85e5\u5ee0\u65bc\u671f\u9650\u5167\u56de\u6536\u7522\u54c1\u4e26\u56de\u5831\u672c\u7f72\uff0c\u4e0d\u5f97\u65bc\u5e02\u9762\u6d41\u901a\u3002\u85e5\u5ee0\u4e26\u61c9\u65bc\u671f\u9650\u5167\u63d0\u51fa\u6539\u5584\u5831\u544a\u53ca\u77ef\u6b63\u9810\u9632\u63aa\u65bd\uff0c\u4e26\u7d93\u672c\u7f72\u78ba\u8a8d\u5f8c\uff0c\u8a72\u5ee0\u540d\u55ae\u59cb\u53ef\u4e0b\u67b6\u3002\u672c\u8cc7\u6599\u96c6\u540c\u6b65\u7f72\u7db2\u66f4\u65b0\uff0c\u85c9\u6b64\u63d0\u4f9b\u6d88\u8cbb\u8005\u3001\u696d\u8005\u6216\u5f9e\u4e8b\u91ab\u7642\u4eba\u54e1\u9032\u4e00\u6b65\u4f7f\u7528\u3002\r\n        "}]